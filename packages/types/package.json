{
  "name": "@batoanng/types",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@batoanng/eslint-config": "workspace:*",
    "@batoanng/tsconfig": "workspace:*",
    "tsup": "^7.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
